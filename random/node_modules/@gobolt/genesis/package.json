{
  "name": "@gobolt/genesis",
  "version": "0.2.14",
  "description": "genesis design system",
  "author": "gobolt",
  "license": "MIT",
  "repository": "SecondCloset/genesis",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.cjs",
      "default": "./dist/components/*.js"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js",
      "require": "./dist/providers/*.cjs",
      "default": "./dist/providers/*.js"
    },
    "./styles/*": {
      "types": "./dist/styles/*.d.ts",
      "import": "./dist/styles/*.js",
      "require": "./dist/styles/*.cjs",
      "default": "./dist/styles/*.js"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.ts",
      "import": "./dist/constants/*.js",
      "require": "./dist/constants/*.cjs",
      "default": "./dist/constants/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs",
      "default": "./dist/utils/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean-install": "npm install --no-audit --no-fund",
    "dev": "vite",
    "build": "tsc --extendedDiagnostics --p ./tsconfig-build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "start": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "generate": "tsx ./lab/generate/generate-component --name",
    "generate:story": "tsx ./lab/generate/generate-story --name",
    "test:watch": "vitest",
    "type": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@eslint/config-array": "latest",
    "@eslint/object-schema": "latest",
    "@fontsource/inter": "^5.1.1",
    "@fontsource/roboto-mono": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-a11y": "^8.6.5",
    "@storybook/addon-docs": "^8.6.5",
    "@storybook/addon-essentials": "^8.6.5",
    "@storybook/addon-interactions": "^8.6.5",
    "@storybook/addon-links": "^8.6.5",
    "@storybook/addon-mdx-gfm": "^8.6.5",
    "@storybook/addon-themes": "^8.6.5",
    "@storybook/blocks": "^8.6.5",
    "@storybook/manager-api": "^8.6.5",
    "@storybook/react": "^8.6.5",
    "@storybook/react-vite": "^8.6.5",
    "@storybook/test": "^8.6.5",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/theming": "^8.6.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "chalk": "^5.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "jsdom": "^23.0.1",
    "msw": "^2.7.1",
    "msw-storybook-addon": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-dts": "^6.1.1",
    "storybook": "^8.6.5",
    "tsx": "^4.19.2",
    "typescript": "^5.2.2",
    "vite": "^6.2.2",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@mdx-js/react": "^3.0.1",
    "@secondcloset/types": "^3.4.6",
    "antd": "^5.23.1",
    "clsx": "^1.2.1",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "debounce": "^2.2.0",
    "react-error-boundary": "^4.0.11",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.2.0",
    "react-toastify": "^11.0.3",
    "recharts": "^2.15.1",
    "styled-components": "^6.0.0",
    "typescript": "^5.2.2"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rollup",
    "react",
    "react component",
    "rollup react",
    "component",
    "example react component",
    "example rollup react component"
  ],
  "bugs": {
    "url": "https://github.com/SecondCloset/genesis/issues"
  },
  "homepage": "https://github.com/SecondCloset/genesis#readme",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "resolutions": {
    "glob": "^10.3.10",
    "rimraf": "^5.0.5"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  }
}
